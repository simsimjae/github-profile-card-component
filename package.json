{
	"name": "github-profile-card-component",
	"version": "1.0.13",
	"description": "minimal github profile card",
	"types": "lib/ProfileCard.d.ts",
	"main": "lib/ProfileCard.js",
	"scripts": {
		"clean": "rm -rf ./lib",
		"tsc": "tsc",
		"bundle": "parcel build ./src/index.tsx --no-content-hash --out-dir ./lib/scripts",
		"build": "yarn clean && tsc && yarn bundle",
		"start": "parcel ./src/index.html"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/simsimjae/github-profile-card.git"
	},
	"keywords": [
		"profile",
		"component",
		"react",
		"github",
		"markdown",
		"readme"
	],
	"author": {
		"name": "Sim Jae Cheol",
		"email": "simsimjae@naver.com",
		"url": "https://github.com/simsimjae"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/simsimjae/github-profile-card/issues"
	},
	"homepage": "https://github.com/simsimjae/github-profile-card#readme",
	"dependencies": {
		"axios": "^0.20.0",
		"module-to-cdn": "^3.1.5",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-google-font-loader": "^1.1.0",
		"react-icons": "^3.11.0",
		"styled-components": "^4.2.2"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/styled-components": "^4.4.2",
		"parcel-bundler": "^1.12.4",
		"tsc": "^1.20150623.0",
		"typescript": "^4.0.2"
	},
	"peerDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	}
}
